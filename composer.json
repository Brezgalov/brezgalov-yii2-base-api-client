{
  "name": "brezgalov/yii2-base-api-client",
  "description": "base component for api clients",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Oleg Brezgalov",
      "email": "oleg.brezgalov@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "yiisoft/yii2": "~2.0.14",
    "yiisoft/yii2-httpclient": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "Brezgalov\\BaseApiClient\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
